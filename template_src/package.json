{
  "name": "cordova-template-vue-src",
  "displayName": "CordovaWithVue",
  "version": "0.1.0",
  "description": "A cordova template with vuejs2.x for creating cordova app.",
  "keywords": [
    "ecosystem:cordova",
    "ecosystem:vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "ls-lint": "ls-lint",
    "stylelint:fix": "stylelint --fix src/**/*.{sass,scss,vue,css,less,stylus}"
  },
  "dependencies": {
    "core-js": "3.6.4",
    "normalize.css": "8.0.1",
    "vue": "2.6.11",
    "vue-router": "3.1.6",
    "vuex": "3.1.3"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "1.9.2",
    "@vue/cli-plugin-babel": "4.3.0",
    "@vue/cli-plugin-eslint": "4.3.0",
    "@vue/cli-plugin-router": "4.3.0",
    "@vue/cli-plugin-unit-jest": "4.3.0",
    "@vue/cli-plugin-vuex": "4.3.0",
    "@vue/cli-service": "4.3.0",
    "@vue/eslint-config-standard": "5.1.2",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "10.1.0",
    "cordova-android": "8.1.0",
    "cordova-plugin-whitelist": "1.3.4",
    "eslint": "6.7.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "6.2.2",
    "less": "3.0.4",
    "less-loader": "5.0.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "speed-measure-webpack-plugin": "1.3.3",
    "style-resources-loader": "1.3.3",
    "stylelint": "13.7.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-selector-bem-pattern": "2.1.0",
    "stylelint-webpack-plugin": "2.1.1",
    "vue-cli-plugin-style-resources-loader": "0.1.4",
    "vue-template-compiler": "2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{sass,scss,vue,css,less,stylus}": [
      "stylelint --fix",
      "git add"
    ],
    "*": [
      "ls-lint"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    }
  }
}
